{
  "name": "react-ability",
  "version": "1.0.7",
  "description": "React permissions based on the BCAC spec",
  "main": "index.js",
  "jest": {
    "testPathIgnorePatterns": [
      "/lib",
      "/config"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupTests.js"
  },
  "scripts": {
    "clean": "rimraf lib",
    "test": "NODE_ENV=test jest --coverage",
    "build": "npm run clean && babel src/ -d lib/",
    "prepublish": "yarn run build"
  },
  "keywords": [
    "react",
    "permissions",
    "ability",
    "authorization",
    "roles"
  ],
  "author": "Jay Shaffer",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "binary-search-tree": "^0.2.6",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint": "^5.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "flow-bin": "^0.79.1",
    "jest": "^23.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "rimraf": "^2.6.2",
    "yarn": "^1.9.4"
  },
  "files": [
    "README.md",
    "index.js",
    "lib/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jayshaffer/react-ability"
  }
}
